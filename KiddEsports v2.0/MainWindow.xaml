<Window x:Class="KiddEsports_v2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KiddEsports_v2._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="290" Width="200">

    <Grid>
        <Image Source="/Binah_Stare.png"/>
        <StackPanel>
            <StackPanel.Resources>
                <Style TargetType="Button">
                    <Setter Property="Margin" Value="0, 5, 0, 0"/>
                </Style>
            </StackPanel.Resources>
            <Label Content="Kidd Esport">
            </Label>
            <Button x:Name="btnPrimaryContacts" Content="Primary Contacts" Height="30" Click="btnPrimaryContacts_Click"/>
            <Button x:Name="btnTeams" Content="Teams" Height="30" Click="btnTeams_Click"/>
            <Button x:Name="btnGames" Content="Games" Height="30" Click="btnGames_Click"/>
            <Button x:Name="btnEventLocations" Content="Event" Height="30" Click="btnEventLocations_Click"/>
            <Button x:Name="btnResults" Content="Results" Height="30" Click="btnResults_Click"/>
            <Button x:Name="btnReports" Content="Reports" Height="30" Click="btnReports_Click"/>
        </StackPanel>
    </Grid>
</Window>
